{
  "name": "wikiloop-battlefield",
  "version": "4.0.1-beta",
  "description": "A web app for Wikipedians to fight vandalism together.",
  "author": "Zainan Victor Zhou <zzn@google.com>",
  "repository": "git@github.com:google/wikiloop-battlefield.git",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "prod": "npm run build && npm run start",
    "build": "nuxt build",
    "analyze": "nuxt build -a",
    "start": "cross-env NODE_ENV=production npx ts-node -r tsconfig-paths/register --project tsconfig.json server/index.ts",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "heroku-postbuild": "npm run build",
    "test-report": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "ci": "circleci local execute build",
    "dev:exec": "ts-node -r tsconfig-paths/register --project tsconfig.json ./server/index.ts",
    "dev:server": "nodemon -w server -w shared -x ts-node ./server/index.ts --server-only"
  },
  "dependencies": {
